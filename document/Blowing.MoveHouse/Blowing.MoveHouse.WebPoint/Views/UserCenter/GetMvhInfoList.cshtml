@{
    Layout = null;
}

@using Blowing.MoveHouse.Model.MoveHouse;
@model IList<MvhInfoModel>
<div> 

    <table>

        <thead>
               <tr>
                   <td>标题</td>
                   <td>发布人</td>
                   <td>地点</td>
                   <td>联系电话</td>
                   <td>最低预算</td>
                   <td>描述</td>
               </tr>
        </thead>

        @foreach (var mvhItem in Model)
        {
            <tr>
                <td>@mvhItem.F_Bj_Title</td>
                <td>@mvhItem.F_Name</td>
                <td>@mvhItem.F_Bj_Site</td>
                <td>@mvhItem.F_Mobile</td>
                <td>@mvhItem.F_BjCostStart</td>
                <td>@mvhItem.F_BjDecription</td>
            </tr>
        }
    </table>
</div>

