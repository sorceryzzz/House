@section Scripts{
    <script type="text/javascript">
        $(function () {
            GetMvhInfoList(1);
        });
        //获取积分明细信息
        function GetMvhInfoList(pageIndex) {
            $.ajax({
                url: '/MvhInfo/GetMvhInfoList?pageIndex=' + pageIndex + "&pageSize=10&startTime=",
                type: 'GET',
                success: function (data) {
                    $("#MvhInfoListBody").html(data);
                }
            });
        }
    </script>}
